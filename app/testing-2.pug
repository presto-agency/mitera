extends layouts/default
block content
  section.testing-page-banner
    <video autoplay muted loop class="bgVideo dec-video">
    <source src="images/bg_video_banner.mp4" type="video/mp4">
    </video>
    .container
      include blocks/_breadcrumbs
      .testing-page-banner__title(
          data-aos="fade-up"
          data-aos-delay="0"
          data-aos-duration="1000")
        h1.section-title.aos-init
          span How Mitera Home Tests Work
        .testing-page-banner__btn.aos-init
          a(href='#').btn
            .btn__bg
            span Choose Your Test
              <svg class="svg">
              <use xlink:href="images/sprite.svg#ic_arrow_right">
              </use>
              </svg>
      .testing-page-banner__img.aos-init(
        data-aos="fade"
        data-aos-duration="1000"
        data-aos-offset="-200")
        .partners.aos-init
          .partners__item
            .partner
              .partner__icon
                img(src='images/ic_how_1.svg' alt loading='lazy')
              .partner__title
                | 1. Request your test order
                .partner__title-icon
                .partner__tooltip Bring value-based maternity care and advanced fetal imaging to your facility with our national network of MFM providers.

          .partners__item
            .partner
              .partner__icon
                img(src='images/ic_how_2.svg' alt loading='lazy')
              .partner__title
                | 2. Provide your sample from home
                .partner__title-icon
                .partner__tooltip Bring value-based maternity care and advanced fetal imaging to your facility with our national network of MFM providers.

          .partners__item
            .partner
              .partner__icon
                img(src='images/ic_how_3.svg' alt loading='lazy')
              .partner__title
                | 3. Receive
                | your results
                .partner__title-icon
                .partner__tooltip Bring value-based maternity care and advanced fetal imaging to your facility with our national network of MFM providers.

        img(src='images/test/test-banner-1.png' alt loading='lazy')
